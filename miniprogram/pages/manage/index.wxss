/* miniprogram/pages/manage/index.wxss */
.photo_bg{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
}
.box{
  width: 100%;
}
.slide_box{
  width: 100%;
  height: auto;
}
.star_container{
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 9;
}
.photo_list_container::after{
  content: "";
  display: block;
  clear: both;
}
.star_container1{
  transform: translateY(0vh);
  animation: slideDown1 10s linear infinite;
}
.star_container2{
  transform: translateY(-100vh);
  animation: slideDown2 10s linear infinite;
}
.star_container .star{
  width: 50rpx;
  height: 50rpx;
}
.photo_list_container{
  width: 100%;
}
.photo_list_item{
  float: left;
}
.border_box{
  border: 10rpx solid #444;
  padding: 20rpx 20rpx 60rpx;
  background-color: #fff;
  position: relative;
}
.photo_list_item image{
  width: 300rpx;
}
.photo_list_item .title{
  position: absolute;
  font-size: 24rpx;
}
@keyframes slideDown1{
  from{
    transform: translateY(0vh)
  }
  to{
    transform: translateY(100vh)
  }
}
@keyframes slideDown2{
  from{
    transform: translateY(-100vh)
  }
  to{
    transform: translateY(0vh)
  }
}

.star_box{
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
}